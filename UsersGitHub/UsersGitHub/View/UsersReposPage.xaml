<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UsersGitHub.View.UsersReposPage"
             xmlns:pages="clr-namespace:UsersGitHub.View"
             xmlns:viewModel="clr-namespace:UsersGitHub.ViewModel;assembly=UsersGitHub"
             IsPresented="{Binding IsPresented, Mode=TwoWay}">
    <MasterDetailPage.BindingContext>
        <viewModel:UsersReposViewModel/>
    </MasterDetailPage.BindingContext>
    <MasterDetailPage.Master>
    <pages:UsersReposPageMaster />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:Users />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>